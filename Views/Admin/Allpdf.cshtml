@{
    ViewData["Title"] = "All pdf";
}

<h1>"All pdf Geography</h1>
</br>

@model IEnumerable<researchOnlineWebsite.Models.ResearchContent>
@{
    ViewData["Title"] = "All Geography Research";
}



<table class="table table-striped">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ResearchPdf)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Specialize)
            </th>

            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.ResearchPdf)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Specialize)
                </td>
                <td>
                    <a class="btn btn-danger mr-3" asp-for="ResearchId" asp-controller="Admin" asp-action="redirectToStudent" asp-route-ResearchId="@item.ResearchId">redirect to student</a>

                </td>

            </tr>
        }
    </tbody>
</table>




